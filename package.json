{
  "private": true,
  "scripts": {
    "ts-node": "node -r ts-node/register/transpile-only",
    "start": "yarn -s ts-node node_modules/.bin/probot run lib/app.ts",
    "precommit": "yarn && yarn test && yarn compile && yarn lint:fix",
    "test": "jest --passWithNoTests",
    "compile": "tsc --noEmit",
    "eslint": "eslint . --ext .ts",
    "prettier": "prettier --check '**/*'",
    "lint": "yarn -s eslint && yarn -s prettier",
    "lint:fix": "yarn -s eslint --fix && yarn -s prettier --write"
  },
  "dependencies": {
    "probot": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
